{
  "name": "alice-semantic-bridge",
  "description": "Advanced n8n nodes for semantic search with pgvector, featuring RAG capabilities, hybrid search, and Redis caching",
  "version": "2.0.0",
  "type": "n8n-nodes",
  "mcp": {
    "enabled": true,
    "server": "asb-cli",
    "autoStart": true
  },
  "features": {
    "pgvector": true,
    "redis": true,
    "openai": true,
    "webscraping": true,
    "rag": true,
    "hybridSearch": true
  },
  "tools": [
    {
      "name": "asb_status",
      "description": "Get project status and configuration"
    },
    {
      "name": "asb_search",
      "description": "Search using pgvector semantic search"
    },
    {
      "name": "asb_embed",
      "description": "Generate text embeddings with OpenAI"
    },
    {
      "name": "asb_webscrape",
      "description": "Scrape and process web content"
    },
    {
      "name": "asb_workflow",
      "description": "Manage n8n workflows"
    },
    {
      "name": "asb_database",
      "description": "PostgreSQL/pgvector operations"
    },
    {
      "name": "asb_redis",
      "description": "Redis cache management"
    },
    {
      "name": "asb_test",
      "description": "Run project tests"
    },
    {
      "name": "asb_build",
      "description": "Build and deploy operations"
    },
    {
      "name": "asb_config",
      "description": "Configuration management"
    }
  ],
  "resources": [
    {
      "uri": "asb://status",
      "name": "Project Status",
      "type": "json"
    },
    {
      "uri": "asb://config",
      "name": "Configuration",
      "type": "json"
    },
    {
      "uri": "asb://workflows",
      "name": "Available Workflows",
      "type": "json"
    },
    {
      "uri": "asb://database/stats",
      "name": "Database Statistics",
      "type": "json"
    }
  ],
  "scripts": {
    "build": "npm run build",
    "test": "npm test",
    "deploy": "npm run deploy:prepare",
    "mcp:start": "node C:\\mcp-servers\\asb-cli\\index.js",
    "mcp:test": "node C:\\mcp-servers\\asb-cli\\test.js"
  },
  "dependencies": {
    "pgvector": "^8.11.3",
    "redis": "^5.3.2",
    "openai": "^4.0.0",
    "n8n": ">=1.0.0"
  }
}
