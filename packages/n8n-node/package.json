{
  "name": "@alice/semantic-bridge-n8n",
  "version": "1.0.0",
  "description": "N8N node for semantic search operations",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "tsc && npm run build:icons",
    "build:icons": "gulp build:icons",
    "watch": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AliceSemanticBridgeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AliceSemanticBridge.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/pg": "^8.15.5",
    "eslint": "^8.57.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.25.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-cloud/documentai": "^8.0.0",
    "@supabase/supabase-js": "^2.39.7",
    "cheerio": "^1.0.0-rc.12",
    "openai": "^4.26.1",
    "p-limit": "^3.1.0",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "puppeteer": "^22.0.0"
  }
}
