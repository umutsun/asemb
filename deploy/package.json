{
  "name": "n8n-nodes-alice-semantic-bridge",
  "version": "1.0.0",
  "description": "Advanced n8n nodes for semantic search with pgvector, featuring RAG capabilities, hybrid search, and Redis caching",
  "keywords": [
    "n8n-community-node",
    "pgvector",
    "postgres",
    "redis",
    "rag",
    "embeddings",
    "semantic-search",
    "hybrid-search",
    "vector-database",
    "openai",
    "web-scraping"
  ],
  "license": "MIT",
  "author": "Alice",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/alice-semantic-bridge.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "npm run clean && npm run build",
    "dev": "tsc -w -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build:prod",
    "deploy:prepare": "npm run build:prod && node scripts/prepare-deploy.js",
    "link:local": "npm link && cd ~/.n8n/nodes && npm link n8n-nodes-alice-semantic-bridge",
    "mcp": "node mcp.js",
    "mcp:status": "node mcp.js status",
    "mcp:agents": "node mcp.js agents",
    "mcp:init": "node mcp.js init",
    "mcp:test": "node mcp.js test",
    "mcp:setup": "setup-claude-code-mcp.bat",
    "shared-memory:init": "node init-shared-memory.js",
    "shared-memory:test": "node test-agent-communication.js",
    "/mcp": "node mcp-check.js /mcp",
    "mcp-check": "node mcp-check.js",
    "vibe": "vibe-coding.bat",
    "orchestrator": "node asb-orchestrator.js",
    "asb": "node asb-orchestrator.js",
    "test:mcp": "node test-mcp-config.js",
    "fix:mcp": "fix-mcp.bat",
    "setup:gemini-codex": "setup-gemini-codex-mcp.bat",
    "test:cli-agents": "node test-cli-agents.js",
    "update:roles": "node update-agent-roles-v2.1.js",
    "setup:gemini": "setup-gemini-cli-mcp.bat",
    "setup:codex": "setup-codex-cli-mcp.bat",
    "setup:all": "setup-all-agents.bat",
    "fix:gemini": "fix-gemini-mcp.bat",
    "setup:codex-mcp": "setup-codex-toml.bat",
    "start:session": "start-multi-agent-session.bat"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/AliceSemanticBridge.node.js",
      "dist/nodes/PgHybridQuery.node.js",
      "dist/nodes/PgvectorQuery.node.js",
      "dist/nodes/PgvectorUpsert.node.js",
      "dist/nodes/WebScrapeEnhanced.node.js",
      "dist/nodes/WebScrape.node.js",
      "dist/nodes/TextChunk.node.js",
      "dist/nodes/DocumentProcessor.node.js",
      "dist/nodes/SitemapFetch.node.js",
      "dist/nodes/RedisPublish.node.js",
      "dist/nodes/Dashboard.node.js"
    ],
    "credentials": [
      "dist/credentials/AliceSemanticBridgeApi.credentials.js",
      "dist/credentials/PostgresWithVectorApi.credentials.js",
      "dist/credentials/OpenAIApi.credentials.js",
      "dist/credentials/RedisApi.credentials.js"
    ]
  },
  "peerDependencies": {
    "n8n-core": ">=1.0.0",
    "n8n-workflow": ">=1.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cheerio": "^0.22.35",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.6",
    "jest": "^29.7.0",
    "n8n-core": ">=1.0.0",
    "n8n-workflow": ">=1.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@mozilla/readability": "^0.4.2",
    "chalk": "^5.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "jsdom": "^22.1.0",
    "langchain": "^0.0.123",
    "node-fetch": "^2.6.9",
    "openai": "^4.0.0",
    "p-limit": "^3.1.0",
    "pg": "^8.11.3",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "xml2js": "^0.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
