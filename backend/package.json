{
  "name": "alice-semantic-bridge",
  "version": "1.0.0",
  "description": "Alice Semantic Bridge - AI-powered semantic search and knowledge management system",
  "main": "dist/server.js",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "dev": "nodemon src/server.ts",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:integration": "npx jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:api": "npx jest --testPathPattern=api\\.health",
    "test:prod": "set NODE_ENV=test&& set TEST_TABLE_NAME=rag_data&& npx jest",
    "test:prod:coverage": "set NODE_ENV=test&& set TEST_TABLE_NAME=rag_data&& npx jest --coverage",
    "test:prod:watch": "set NODE_ENV=test&& set TEST_TABLE_NAME=rag_data&& npx jest --watch",
    "migrate": "node scripts/migrate-rag-embeddings.js",
    "extract-entities": "node scripts/extract-entities-batch.js",
    "build": "npm run test && npm run migrate",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "semantic-search",
    "rag",
    "pgvector",
    "ai",
    "knowledge-management"
  ],
  "author": "ASB Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/anthropic": "^0.3.27",
    "@langchain/community": "^0.3.54",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.2.17",
    "@langchain/groq": "^0.2.3",
    "@langchain/openai": "^0.3.17",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query-devtools": "^5.85.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pdf-parse": "^1.1.5",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "chalk": "^4.1.2",
    "cheerio": "^1.1.2",
    "cli-progress": "^3.12.0",
    "cmdk": "^1.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.23.12",
    "groq-sdk": "^0.32.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.33",
    "lru-cache": "^11.1.0",
    "mammoth": "^1.10.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "n8n-workflow": "^1.82.0",
    "neo4j-driver": "^5.28.1",
