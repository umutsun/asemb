    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "k6": "^0.0.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.0.2",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "test/**/*.{js,ts,tsx}": [
      "jest -o --bail --passWithNoTests"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
