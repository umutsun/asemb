    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pgvector": "^0.1.8",
    "pino": "^8.17.2",
    "puppeteer": "^24.20.0",
    "redis": "^4.6.12",
    "socket.io": "^4.8.1",
    "tiktoken": "^1.0.22",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
